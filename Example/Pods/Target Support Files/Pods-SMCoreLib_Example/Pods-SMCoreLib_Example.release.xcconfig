EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/AFNetworking" "$PODS_CONFIGURATION_BUILD_DIR/HPTextViewTapGestureRecognizer" "$PODS_CONFIGURATION_BUILD_DIR/Reachability" "$PODS_CONFIGURATION_BUILD_DIR/SMCoreLib"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/AFNetworking/AFNetworking.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/HPTextViewTapGestureRecognizer/HPTextViewTapGestureRecognizer.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Reachability/Reachability.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SMCoreLib/SMCoreLib.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "AFNetworking" -framework "HPTextViewTapGestureRecognizer" -framework "Reachability" -framework "SMCoreLib"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
